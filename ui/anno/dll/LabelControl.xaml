<UserControl x:Class="ssi.LabelControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="Auto" Width="Auto" AllowDrop="True">
    
    <Grid
        VerticalAlignment="Stretch" 
        HorizontalAlignment="Stretch"
        Width="Auto"
        Height="Auto">
        
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        
        <Grid Grid.Column="0" Grid.Row="1" Name="labelGrid">
        </Grid>

    </Grid>
    
</UserControl>
